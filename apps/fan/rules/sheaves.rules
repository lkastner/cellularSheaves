   property BASES : NodeMap<Directed, Matrix>;
   
   property BLOCKS : EdgeMap<Directed, Matrix>;

}

declare object CoSheaf {
   
   property BASES : NodeMap<Directed, Matrix>;
   
   property BLOCKS : EdgeMap<Directed, Matrix>;
   
}

object PolyhedralComplex {

   property SHEAF : Sheaf : multiple;
   
   property COSHEAF : CoSheaf : multiple;

   property COMPACT_SHEAF : Sheaf : multiple;

   property COMPACT_COSHEAF : CoSheaf : multiple;

}
